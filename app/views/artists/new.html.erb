<h1>Add New Artist</h1>

<% if @errors %>
  <% @errors.each do |error| %>
    <%= error %><br>
  <% end %>
<% end %>

<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.submit %>
<% end %>
<%= render 'shared/end_links' %>